<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="ring" android:innerRadiusRatio="2.2" android:useLevel="false" android:thickness="@dimen/quick_training_target_progress_bar_thickness">
            <solid android:color="@color/training_target_progress_background" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <shape android:shape="ring" android:innerRadiusRatio="2.25" android:useLevel="true" android:thickness="@dimen/quick_training_target_progress_bar_thickness_secondary">
            <solid android:color="@color/black" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <shape android:shape="ring" android:innerRadiusRatio="2.2" android:useLevel="true" android:thickness="@dimen/quick_training_target_progress_bar_thickness">
            <gradient android:startColor="?colorTargetProgressbarStart" android:endColor="?colorTargetProgressbarEnd" android:type="sweep" />
        </shape>
    </item>
</layer-list>