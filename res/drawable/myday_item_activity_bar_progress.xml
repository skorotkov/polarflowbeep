<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <solid android:color="@color/myday_item_activity_bar_progress_background" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <gradient android:startColor="@color/myday_item_activity_bar_progress_start" android:endColor="@color/myday_item_activity_bar_progress_end" android:angle="0.0" />
            </shape>
        </clip>
    </item>
</layer-list>